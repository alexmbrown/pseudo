{
  "name": "pseudo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "coverage": "npm run coverage:client && npm run coverage:electron",
    "coverage:client": "ng test --cc --single-run",
    "coverage:electron": "istanbul cover jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "postcoverage": "istanbul-combine -p summary -r html -r lcov coverage/**/coverage-final.json -d coverage",
    "start": "concurrently \"ng serve\" \"npm run electron\"",
    "electron": "electron ./src/electron/electron.dev",
    "build": "ng build",
    "test": "npm run test:client && npm run test:electron",
    "test:client": "ng test -sr",
    "pretest:client": "npm run lint:client",
    "test:electron": "jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "pretest:electron": "npm run lint:electron",
    "lint": "npm run lint:client && npm run lint:electron",
    "lint:client": "ng lint",
    "lint:electron": "standard --fix --env jasmine",
    "package:win": "npm run build && cross-var electron-packager dist $npm_package_name-$npm_package_version --out=packages --platform=win32 --arch=all --overwrite ",
    "package:linux": "npm run build && cross-var electron-packager dist $npm_package_name-$npm_package_version --out=packages --platform=linux --arch=all --overwrite ",
    "package:mac": "npm run build && cross-var electron-packager dist $npm_package_name-$npm_package_version --out=packages --platform=darwin --arch=all --overwrite ",
    "package:all": "npm run build && cross-var electron-packager dist $npm_package_name-$npm_package_version --out=packages --all --arch=all --overwrite ",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.4.6",
    "@angular/cdk": "^5.0.0-rc0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "^5.0.0-rc0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "ace-builds": "^1.2.9",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.1",
    "cross-var": "^1.1.0",
    "electron": "^1.7.9",
    "express": "^4.16.2",
    "fs-extra": "^4.0.2",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.4",
    "node-sass": "^4.7.2",
    "npm": "^5.5.1",
    "rxjs": "^5.4.2",
    "shelljs": "^0.7.8",
    "socket.io": "^2.0.4",
    "uuid": "^3.1.0",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.4.2",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@types/express": "^4.0.39",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "angular2-template-loader": "^0.6.2",
    "codelyzer": "~3.1.1",
    "concurrently": "^3.5.0",
    "electron-packager": "^9.1.0",
    "electron-reload": "^1.2.2",
    "istanbul": "^0.4.5",
    "istanbul-combine": "https://github.com/sutoiku/istanbul-combine",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-electron": "^5.2.2",
    "karma-electron-launcher": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "material-design-icons": "^3.0.1",
    "protractor": "~5.1.2",
    "rewire": "^3.0.2",
    "standard": "^10.0.3",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.5.0",
    "wallaby-webpack": "^3.9.3"
  }
}
